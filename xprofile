#!/bin/bash

firefox &
# spawn 3 terms
termite --class 'term-main' &
termite --class 'term-main' &
termite --class 'term-main' &
termite --class 'term-main' &
# emacs
emacs -name emacs-main &
# mail
termite --class 'term-mutt' --exec=/usr/bin/neomutt &
setxkbmap us -variant altgr-intl -option ctrl:swapcaps &

feh --bg-fill ~/Syncs/Wallpapers/Stones.jpg &
#exec xmonad
